<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="five88_suite" parallel="methods" thread-count="1" data-provider-thread-count="1">
    <parameter name="suite-name" value="five88_suite"/>

    <test name="five88_test_other">
        <groups>
            <run>
                <include name="OTHER_GRP"/>
            </run>
        </groups>

        <classes>
            <class name="com.five88.scripts.Five88Script"/>
        </classes>
    </test>

    <test name="five88_test_register">
        <groups>
            <run>
                <include name="REG_GRP"/>
            </run>
        </groups>

        <classes>
            <class name="com.five88.scripts.RegisterScript"/>
        </classes>
    </test>

    <test name="five88_test_login">
        <groups>
            <run>
                <include name="LOGIN_GRP"/>
            </run>
        </groups>

        <classes>
            <class name="com.five88.scripts.LoginScript"/>
        </classes>
    </test>

